def stoneGame(n):
    l = [2,1,2]
    m = {0:False,1: True,2:False}
    for i in range(3,n + 1):
        t = False
        m[i] = isAPowerB(i)
        for j in range(1,i + 1):
           if isAPowerB(j) and l[i - j] == 2: 
                l.append(1)
                t = True
                break
        if not t : l.append(2)
    return l[n]
